.avatar-uploader .el-upload {
    border: 1px dashed #d9d9d9;
    border-radius: 6px;
    cursor: pointer;
    position: relative;
    overflow: hidden;
}
.avatar-uploader .el-upload:hover {
    border-color: #409EFF;
}
.avatar-uploader-icon {
    font-size: 28px;
    color: #8c939d;
    width: 178px;
    height: 178px;
    line-height: 178px;
    text-align: center;
}
.avatar {
    width: 178px;
    height: 178px;
    display: block;
}
@media only screen and (max-width: 640px) {
    .el-message-box {
        max-width:264px;
    }
}

.nat-offline{
    color:gray;
}

.import-exports-label{
    line-height: 36px;
    text-align: center;
}
.import-data-dialog-note{
    color:red;
    text-align: center;
}
.import-data-dialog .el-button,.exports-data-dialog .el-button{
    width:100%;
}
.row{
    margin-top:15px;
}
